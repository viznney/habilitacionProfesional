<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="entidades.UsuarioLab" table="usuariolab">
       <id name="OID" type="string">
            <column name="OIDUsuarioLab"/>
            <generator class="uuid2"/>
        </id>

        <property name="contraseña" type="string">
            <column name="contraseña"/>
        </property>
        
        <property name="usuario" type="string">
            <column name="usuario"/>
        </property>
        
        <many-to-one name="m_EstadoUsuario" class="entidades.EstadoUsuario" not-null="true">
            <column name="OIDEstadoUsuario"/>
        </many-to-one>
        
        <many-to-one name="m_PermisoUsuario" class="entidades.PermisoUsuario" not-null="true">
            <column name="OIDPermisoUsuario"/>
        </many-to-one>
        
    </class>
</hibernate-mapping>
